{
  "root": true,
  "extends": "eslint:recommended",
  "plugins": [
    "angular"
  ],
  "env": {
    "node": false,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 5
  },
  "globals": {
    "angular": true,
    "_": true,
    "CKEDITOR": true,
    "d3": true,
    "SVG": true,
    "Uint8Array": true,
    "$": true,
    "require": true,
    "__dirname": true
  },
  "rules": {
    //excluded angular rules
    "angular/controller-as": 0,
    "angular/controller-as-vm": 0,
    "angular/no-service-method": 0,
    "angular/module-getter": 0,
    "angular/json-functions": 0,
    "angular/controller-name": 0,
    "angular/module-setter": 0,
    "angular/definedundefined": 0,
    "angular/typecheck-string": 0,
    "angular/typecheck-number": 0,
    "angular/di": 0,
    "angular/typecheck-function": 0,
    "angular/no-services": 0,
    "angular/no-digest": 0,
    "angular/window-service": 0,
    "angular/typecheck-array": 0,
    "angular/no-private-call": 0,
    "angular/angularelement": 0,
    //Angular rules - enabled
    "angular/controller-as-route": 2,
    "angular/deferred": 0,
    "angular/di-order": [
      0,
      true
    ],
    "angular/directive-name": 0,
    "angular/component-limit": [
      0,
      1
    ],
    "angular/document-service": 2,
    "angular/empty-controller": 0,
    "angular/file-name": 0,
    "angular/filter-name": 0,
    "angular/foreach": 0,
    "angular/function-type": 0,
    "angular/interval-service": 2,
    "angular/log": 2,
    "angular/module-name": 0,
    "angular/no-angular-mock": 0,
    "angular/no-controller": 0,
    "angular/no-cookiestore": 2,
    "angular/no-jquery-angularelement": 2,
    "angular/on-watch": 2,
    "angular/rest-service": 0,
    "angular/service-name": 2,
    "angular/timeout-service": 2,
    "angular/typecheck-date": 2,
    "angular/typecheck-object": 2,
    "angular/watchers-execution": [
      0,
      "$digest"
    ],
    //js rules

    "no-console": 2,
    "no-process-env": 2

























    //to-fix






    //curently disabled
  }
}
